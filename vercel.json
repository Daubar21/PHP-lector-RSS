{
  "functions": {
    "api/*.php": { "runtime": "vercel-php@0.6.0" }
  },
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php@0.6.0"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}
